{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "./",
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "types": ["express"],
  "include": ["src/**/*.ts", "populatedb.ts"],
  "exclude": ["node_modules"]
}
